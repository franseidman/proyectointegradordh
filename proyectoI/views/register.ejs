<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="./images/logo-mercado-liebre.jpg" />

	<title> Mercado Liebre Argentina</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="./css/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="./css/app.css">
</head>

<body>
	<!-- Header -->
	<%- include('./partials/header.ejs')%>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<div class="todo">
				<form id="ingresoform" name="ingfor">
					<fieldset>
						<legend id="titulo" class="Llc">Información de Registro</legend>
						<div class="marcd" id="primdiv">
							<label class="Llc">Nombre de usuario:</label>
							<input type="text" name="nom" id="n1">
						</div>
						<div class="marcd">
							<label class="Llc">Correo:</label>
							<input type="text" name="co" id="c1">
						</div>
						<div class="marcd">
							<label class="Llc">Constraseña:</label>
							<input type="password" name="co" id="c1">
						</div>
						<div class="marcd">
							<label class="Llc">Fecha de nacimiento:</label>
							<input type="date" name="co" id="c1">
						</div>
						<div class="marcd">
							<label class="Llc">Teléfono:</label>
							<input type="number" name="co" id="c1">
						</div>
						<input type="submit" class="no" id="botonFin" value="Registrarse" onmouseover="validarUlti()" onclick="Login()">
					</fieldset>
				</form>
				</div>
			</h2>


	<!-- footer -->
	<%- include('./partials/footer.ejs')%>
	<!-- /footer -->
</body>

</html>